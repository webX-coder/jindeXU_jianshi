body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote, th, td {
    margin: 0;padding: 0;
}
h1, h2, h3, h4, h5, h6{
    font-weight:normal;
}
html {
    -webkit-text-size-adjust: none;
}
body {
    padding: 0;
    margin: 0;
    -webkit-font-smoothing: antialiased;
    font: 12px/1.5 Microsoft YaHei,tahoma,arial,Hiragino Sans GB,\\5b8b\4f53,sans-serif !important;
    color: #666;
    font-size: 14px;
    background-color: #14669c !important;      
}
* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing:
    border-box;-moz-box-sizing:
    border-box;box-sizing:
    border-box;
}
*:before, *:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
a {
    text-decoration: none;
    background: transparent;
    -webkit-tap-highlight-color: transparent
}
a:hover,a{
    text-decoration:none;
}
b,i,em{
    font-style:normal;
}
ul{
    padding:0;
    list-style:none;
}
ul,li {
    margin: 0;
    padding: 0;
    list-style-type: none
}
img {
    border: none;
    vertical-align: middle
}
button {
    cursor: pointer
}
input{
    outline: none;
}
button,input {
    font-family: Microsoft YaHei,tahoma,arial,Hiragino Sans GB,\\5b8b\4f53,sans-serif
}
div,span,a,header {
    -webkit-tap-highlight-color: rgba(255,255,255,0)
}
/*头部导航*/
    .clearfix:after {
        content: "";
        display: block;
        visibility: hidden;
        height: 0;
        clear: both;
    }

    .clearfix {
        zoom: 1;
    }
polyline {
    fill: none;
    stroke: #B0D3FC;
    stroke-width: 1px;
    stroke-dasharray: 0px;
}
.navList a{
    display: inline-block;
    width: 100%;
    height: 100%;
}
@media (max-width: 1900px){
    .yx_nav, .yx-control-center, .xy_electronic-commerce, .yx_agriculture-science, .yx_tea-brigade, .yx_demonstration-base, .community-service,.yx_agriculture-science ul{
        width: 1920px !important;
    }
}
@media (max-width: 1366px){
    .yx_tea-brigade_alert{
    	left:387px;
    }
}
