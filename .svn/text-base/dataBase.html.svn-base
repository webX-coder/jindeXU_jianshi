<script>
    function html(item){
        return'<b class="yx_video_close" onclick=videoClose()></b>'+
            '<h3>'+item.name+'</h3>'+
            '<p>发布时间：'+item.time+'<span>来源： '+item.source+'</span></p>'+
            '<div style="width: 1100px;height: 1px;background-color: #61afd1;margin:0 auto;margin-top: 30px;"></div>'+
            '<em>'+item.briefIntro+'</em>'+
            '<img src="'+item.img+'" alt="">'
    }
	$(".left-activity li").click(function(){
		$(this).addClass("active").siblings().removeClass("active");
        var _this = $(this)
        $(".yx_alert").show();
        $.getJSON('static/front/data/expert.json', function (data) {
            data.data.forEach(function(item,index){
                if(item.id==_this.attr('typeId')){
                    $('.yx_alert').html(html(item))
                }
            })
        });
	})
	$(".right-dataBase li").click(function(){
		$(this).addClass("active").siblings().removeClass("active");		
	})
    function videoClose() {
        $(".yx_alert").hide();
    }
</script>
